        }","        ","        completionHandler()","    }","}"],"fileType":"swift","highlights":[{"line":3},{"line":9},{"line":10},{"line":11},{"line":12},{"line":13},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30},{"line":31}],"fileName":"AppDelegate.swift"},"flutter.app_delegate_02.swift.sample":{"identifier":"flutter.app_delegate_02.swift.sample","fileName":"AppDelegate.swift","type":"file","syntax":"swift","highlights":[{"line":2},{"line":9},{"line":24},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40},{"line":41},{"line":42},{"line":43},{"line":44},{"line":45},{"line":46},{"line":47},{"line":48},{"line":49}],"content":["import UIKit","import UserNotifications","import Flutter","import JivoSDK","","@UIApplicationMain","@objc class AppDelegate: FlutterAppDelegate {","    override func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","        UNUserNotificationCenter.current().delegate = self","        GeneratedPluginRegistrant.register(with: self)","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        return super.application(application, didFinishLaunchingWithOptions: launchOptions)","    }","    ","    override func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    override func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(data: nil)","    }","    ","    override func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    override func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        if Jivo.notifications.didReceive(response: response) {","            \/\/ JivoSDK would like to appear onscreen here","        }","        ","        completionHandler()","    }","}"],"fileType":"swift"},"flutter.profile_widget_02.dart.sample":{"identifier":"flutter.profile_widget_02.dart.sample","type":"file","syntax":"dart","content":["import 'package:flutter\/material.dart';","import 'package:jivosdk_plugin\/bridge.dart';","","class ProfileWidget extends StatelessWidget {","    const ProfileWidget({super.key});","    ","    @override","    Widget build(BuildContext context) {","        return Scaffold(","            appBar: AppBar(","                title: const Text('Profile')","            ),","            floatingActionButton: FloatingActionButton(","                child: const Icon(Icons.support),","                onPressed: () {","                    Jivo.display.present();","                }","            ),","        );","    }","}"],"fileType":"dart","highlights":[{"line":2},{"line":16}],"fileName":"ProfileWidget.dart"},"flutter.app_delegate_00.swift.sample":{"identifier":"flutter.app_delegate_00.swift.sample","fileName":"AppDelegate.swift","type":"file","syntax":"swift","highlights":[],"content":["import UIKit","import Flutter","","@UIApplicationMain","@objc class AppDelegate: FlutterAppDelegate {","    override func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","        GeneratedPluginRegistrant.register(with: self)","        return super.application(application, didFinishLaunchingWithOptions: launchOptions)","    }","}"],"fileType":"swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-customize-UI-elements":{"url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-customize-UI-elements","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-customize-UI-elements","type":"section","title":"How to customize UI elements","role":"pseudoSymbol","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"kind":"section"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics#How-to-display-SDK":{"role":"pseudoSymbol","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics#How-to-display-SDK","type":"section","kind":"section","url":"\/tutorials\/jivosdk\/native_usage_basics#How-to-display-SDK","title":"How to display SDK","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}]},"doc://