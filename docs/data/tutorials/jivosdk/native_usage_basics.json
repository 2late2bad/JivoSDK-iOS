ft","identifier":"native.app_delegate_02.swift.sample","fileName":"AppDelegate.swift","type":"file","content":["import Foundation","import UIKit","import UserNotifications","import JivoSDK","","final class AppDelegate: UIResponder",", UIApplicationDelegate",", UNUserNotificationCenterDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        UNUserNotificationCenter.current().delegate = self","        ","        if Jivo.notifications.handleLaunch(options: launchOptions) {","            \/\/ Launched by JivoSDK notification","        }","        ","        return true","    }","    ","    func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(error: error)","    }","    ","    func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            \/\/ Notification for JivoSDK, just pass this result into completionHandler","            completionHandler(result)","        }","        else {","            \/\/ Notification for your app, provide the result needed for your business logic","            completionHandler(.noData)","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        if let options = Jivo.notifications.willPresent(notification: notification, preferableOptions: .banner) {","            \/\/ Notification for JivoSDK, just pass these options into completionHandler","            completionHandler(options)","        }","        else {","            \/\/ Notification for your app, provide the options needed for your business logic","            completionHandler([])","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        Jivo.notifications.didReceive(response: response)","        completionHandler()","    }","}"]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics#How-to-handle-user-session":{"abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"kind":"section","url":"\/tutorials\/jivosdk\/react_usage_basics#How-to-handle-user-session","type":"section","title":"How to handle user session","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics#How-to-handle-user-session","role":"pseudoSymbol"},"doc://org.cocoapods.JivoSDK/documentation/JivoSDK/common_user_token":{"kind":"article","identifier":"doc:\/\/org.cocoapods.JivoSDK\/documentation\/JivoSDK\/common_user_token","abstract":[{"type":"text","text":"Required for keeping chat history alive"}],"url":"\/documentation\/jivosdk\/common_user_token","role":"article","title":"User Token and JWT","type":"topic"},"doc://org.cocoapods.JivoSDK/tutorials/table":{"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table","kind":"overview","type":"topic","role":"overview","title":"JivoSDK Samples","url":"\/tutorials\/table","abstract":[]},"doc://org.cocoapods.JivoSDK/documentation/JivoSDK/JVDisplayElement":{"identifier":"doc:\/\/org.cocoapods.JivoSDK\/documentation\/JivoSDK\/JVDisplayElement","abstract":[{"type":"text","text":"UI elements available for customization"}],"role":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","t