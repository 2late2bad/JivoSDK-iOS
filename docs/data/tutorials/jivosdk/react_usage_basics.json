nt notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        Jivo.notifications.configurePresentation(notification: notification, proxyTo: completionHandler) { target, event in","            switch target {","            case .app:","                return []","            case .sdk:","                return [.alert]","            }","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        Jivo.notifications.handleUser(response: response)","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics":{"type":"topic","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics","role":"project","kind":"project","url":"\/tutorials\/jivosdk\/react_usage_basics","title":"Basic usage"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/flutter_usage_basics#How-to-handle-Push-Notifications":{"type":"section","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/flutter_usage_basics#How-to-handle-Push-Notifications","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"role":"pseudoSymbol","kind":"section","url":"\/tutorials\/jivosdk\/flutter_usage_basics#How-to-handle-Push-Notifications","title":"How to handle Push Notifications"},"doc://org.cocoapods.JivoSDK/tutorials/table/Native-app":{"type":"topic","abstract":[],"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/table\/Native-app","role":"article","kind":"article","url":"\/tutorials\/table\/native-app","title":"Native app"}},"variantOverrides":[{"traits":[{"interfaceLanguage":"occ"}],"patch":[{"op":"replace","