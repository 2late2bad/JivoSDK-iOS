ort FlipperKit","#endif","#endif","","@UIApplicationMain","final class AppDelegate: UIResponder",", UIApplicationDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil) -> Bool {","        initializeFlipper(with: application)","        ","        \/\/ ...","        ","        Jivo.notifications.handleLaunch(options: launchOptions)","        return true","    }","    ","    func application(_ application: UIApplication, didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Jivo.notifications.setPushToken(data: deviceToken)","    }","    ","    func application(_ application: UIApplication, didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Jivo.notifications.setPushToken(error: error)","    }","    ","    func application(_ application: UIApplication, didReceiveRemoteNotification userInfo: [AnyHashable : Any], fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        if let result = Jivo.notifications.didReceiveRemoteNotification(userInfo: userInfo) {","            completionHandler(result)","        }","        else {","            completionHandler(.noData)","        }","    }","","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"],"highlights":[{"line":3},{"line":19},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39}],"syntax":"swift","fileName":"AppDelegate.swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics":{"role":"project","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics","type":"topic","title":"Basic usage","url":"\/tutorials\/jivosdk\/native_usage_basics","kind":"project","abstract":[{"type":"text","text":"Some