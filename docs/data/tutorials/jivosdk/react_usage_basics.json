            case .sdk:","                return [.alert]","            }","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        Jivo.notifications.handleUser(response: response)","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"],"fileName":"AppDelegate.swift","identifier":"react.app_delegate_02.swift.sample","type":"file","fileType":"swift","highlights":[{"line":3},{"line":14},{"line":15},{"line":22},{"line":42},{"line":43},{"line":44},{"line":45},{"line":46},{"line":47},{"line":48},{"line":49},{"line":50},{"line":51},{"line":52},{"line":53},{"line":54},{"line":55},{"line":56},{"line":57}]},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/native_usage_basics":{"role":"project","title":"Basic usage","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/native_usage_basics","kind":"project","type":"topic","url":"\/tutorials\/jivosdk\/native_usage_basics"}},"variantOverrides":[{"patch":[{"op":"replace","path":"\/identifier\/interfaceLanguage","value":"occ"}],"traits":[{"interfaceLanguage":"occ"}]}]}