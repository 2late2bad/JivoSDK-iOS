","        completionHandler(.noData)","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, willPresent notification: UNNotification, withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void) {","        Jivo.notifications.configurePresentation(notification: notification, proxyTo: completionHandler) { target, event in","            switch target {","            case .app:","                return []","            case .sdk:","                return [.alert]","            }","        }","    }","    ","    func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {","        Jivo.notifications.handleUser(response: response)","        completionHandler()","    }","    ","    private func initializeFlipper(with application: UIApplication) {","        \/\/ ...","    }","}"],"fileType":"swift","syntax":"swift"},"doc://org.cocoapods.JivoSDK/tutorials/JivoSDK/react_usage_basics":{"url":"\/tutorials\/jivosdk\/react_usage_basics","kind":"project","title":"Basic usage","abstract":[{"type":"text","text":"Some scenarios of how to use SDK"}],"type":"topic","identifier":"doc:\/\/org.cocoapods.JivoSDK\/tutorials\/JivoSDK\/react_usage_basics","role":"project"},"react.user_manager_01.js.sample":{"content":["import Jivo from '.\/jivosdk_module'","","class UserManager {","    constructor() {","        this.user = null","    }","","    updateUser(user) {","        this.user = user","        this._configureJivoSession()","    }","","    _configureJivoSession() {","        if (this.user) {","            Jivo.startUp(\"YOUR_CHANNEL_ID\", this.user.supportJwt)","        }","        else {","            Jivo.shutDown()","        }","    }","}"],"syntax":"js","fileName":"UserManager.js","highlights":[{"line":1},{"line":2},{"line":10},{"line":12},{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line"